#builtin("Unit")
struct Unit {}

#builtin("I32")
type I32

#builtin("Runtime")
type Runtime

struct Foo {
    member: Unit,
}

fn main(runtime: Runtime) {
    print_i32(5, runtime)
    print_i32(10, runtime)

    Foo {
        member: {}, // shorter way to construct the Unit type, just make the compiler do it for you
    }.member
}

// below to prove that out-of-order declarations work
#builtin("print_i32")
fn print_i32(value: I32, runtime: Runtime)
